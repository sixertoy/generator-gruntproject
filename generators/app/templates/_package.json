{
    "name": "<%= project.name %>",
    "version": "0.1.0",
    "description": "<%= project.description %>",
    "main": "index.js",
    "scripts": {
        "preinstall": "git init",
        "clean": "rm -rf node_modules",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "echo \"Error: no start specified\" && exit 1",
        "postinstall": "cd .git/hooks && ln -s ../../bin/git/commit-msg.js && mv commit-msg.js commit-msg && cd ../../"
    },
    "engines": {
        "node": ">=<%= node.version %>"
    },
    "repository": {
        "type": "git",
        "url": "<%= project.repository %>"
    },
    "keywords": [
        "grunt"
    ],
    "author": [{
        "name": "<%= author.name %>",
        "email": "<%= author.email %>"
    }],
    "license": "MIT",
    "bugs": {
        "url": "<%= project.repository %>/issues"
    },
    "homepage": "<%= project.repository %>",
    "dependencies": {
        "class-extend": "",
        "lodash": "",
        "object-assign": ""
    },
    "devDependencies": {
        "grunt": "",
        "grunt-bump": "",
        "grunt-contrib-connect": "",
        "grunt-contrib-jshint": "",
        "grunt-contrib-watch": "",
        "grunt-coveralls": "",
        "grunt-jasmine-node-coverage": "",
        "grunt-jsbeautifier": "",
        "grunt-markdown": "",
        "grunt-wrap": "",
        "load-grunt-config": "",
        "sinon": ""
    }
}
